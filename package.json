    "active directory"
  ],
  "bugs": {
    "url": "https://github.com/gheeres/node-activedirectory/issues"
  },
  "dependencies": {
    "async": ">= 0.1.22",
    "bunyan": ">= 1.3.5",
    "ldapjs": "https://github.com/mcavage/node-ldapjs.git#acc1ca8f4314fd9d67561feabc8ce4c235076a5e",
    "underscore": ">= 1.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gheeres/node-activedirectory.git"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^2.2.5"
  }
}
